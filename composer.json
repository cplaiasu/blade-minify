{
  "name": "ken/blade-minify",
  "description": "Laravel Blade Minify",
  "keywords": [
    "laravel",
    "blade",
    "minify",
    "html",
    "litepie"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ken",
      "email": "wahyu.dhiraashandy8@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/support": "5.3.x|5.4.x|5.5.x"
  },
  "autoload": {
    "psr-4": {
      "Ken\\BladeMinify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ken\\BladeMinify\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.2",
    "orchestra/testbench": "3.x",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 src/ tests/",
    "fix-style": "phpcbf -p --standard=PSR2 src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ken\\BladeMinify\\ServiceProvider"
            ]
        }
    }
}
